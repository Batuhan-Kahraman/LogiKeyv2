@{
    var currentPage = ViewContext.RouteData.Values["Controller"].ToString();
}
<!DOCTYPE html>
<html data-navigation-type="default" data-navbar-horizontal-shape="default" lang="en-US" dir="ltr">

<head>
<meta http-equiv="content-type" content="text/html;charset=utf-8" />
    @await Html.PartialAsync("Head")
  </head>

  <body>

    <main class="main" id="top">
    @await Html.PartialAsync("Navbar")
      <div class="content">
            @if (currentPage != "Home")
            {
                <!--<div class="mb-3">
                    <button onclick="window.close()"><i class="far fa-window-close me-1"></i> Geri Dön</button>

                </div>-->
            }
           @RenderBody()
    
      </div>
     
    </main>



    @await Html.PartialAsync("Script")

    @await RenderSectionAsync("Scripts", required: false)
    <script>
        function goBack() {
            window.history.back();
        }
        function close() {
            window.close();
        }
    </script>
  </body>



