@using DataAccessLayer.Concrate
@model EntityLayer.Concrate.UyariTip
@{
    ViewData["Title"] = "Uyarı Tipi Düzenle";
}
<div class="page-titles">
    <ol class="breadcrumb">
        <li><h5 class="bc-title">Uyarı > </h5></li>
        <li class="breadcrumb-item active"><a href="javascript:void(0)">Uyarı Tipi Düzenle</a></li>
    </ol>
</div>
<div class="container-fluid">
    <div class="row">
        <div class="col-xl-9 wid-100">
            <form method="post">

                <div class="row">
                    <div class="mb-3 d-flex">
                        <span style="color:red;">*</span>
                        <label class="form-label">Ad</label>
                        <input class="form-control" asp-for="Ad" required>
                    </div>
                </div>

                <div class="row">
                    <div class="mb-3">
                        <input type="submit" class="form-control" value="Düzenle" />
                    </div>
                </div>
            </form>
        </div>

    </div>
</div>